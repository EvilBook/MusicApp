<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="signUpBackground" prefHeight="430.0" prefWidth="667.0" stylesheets="@CSS/signup.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SignUpController">
   <children>
      <TextField id="firstTextField" fx:id="firstNameTextField" layoutX="67.0" layoutY="119.0" prefHeight="40.0" prefWidth="160.0" promptText="First Name" />
      <TextField id="lastTextField" fx:id="lastNameTextField" layoutX="366.0" layoutY="119.0" prefHeight="40.0" prefWidth="160.0" promptText="Last Name" />
      <TextField fx:id="emailTextField" layoutX="67.0" layoutY="171.0" prefHeight="40.0" prefWidth="160.0" promptText="Email" />
      <TextField fx:id="confirmEmailTextField" layoutX="366.0" layoutY="175.0" prefHeight="40.0" prefWidth="160.0" promptText="Confirm Email" />
      <PasswordField id="passwordTextField" fx:id="passwordPasswordField" layoutX="67.0" layoutY="224.0" prefHeight="40.0" prefWidth="160.0" promptText="Password" />
      <PasswordField id="confirmPasswordTextField" fx:id="confirmPasswordField" layoutX="366.0" layoutY="224.0" prefHeight="40.0" prefWidth="160.0" promptText="Confirm Password" />
      <Text fill="WHITE" layoutX="101.0" layoutY="78.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create a new account" textAlignment="JUSTIFY">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Button fx:id="submitButton" layoutX="481.0" layoutY="346.0" mnemonicParsing="false" onAction="#handleSubmitButton" prefHeight="40.0" prefWidth="105.0" text="Submit" />
      <Button id="returnButton" fx:id="backButton" layoutX="14.0" layoutY="351.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="35.0" prefWidth="105.0" text="Return" />
   </children>
</AnchorPane>
