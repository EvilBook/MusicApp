<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@signup.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SignUpController">
   <children>
      <TextField fx:id="lastNameTextField" layoutX="374.0" layoutY="69.0" prefHeight="40.0" prefWidth="160.0" promptText="Last Name" />
      <TextField fx:id="emailTextField" layoutX="60.0" layoutY="121.0" prefHeight="40.0" prefWidth="160.0" promptText="Email" />
      <TextField fx:id="confirmEmailTextField" layoutX="374.0" layoutY="121.0" prefHeight="40.0" prefWidth="160.0" promptText="Confirm Email" />
      <PasswordField fx:id="passwordPasswordField" layoutX="60.0" layoutY="173.0" prefHeight="40.0" prefWidth="160.0" promptText="Password" />
      <PasswordField fx:id="confirmPasswordField" layoutX="374.0" layoutY="173.0" prefHeight="40.0" prefWidth="160.0" promptText="Confirm Password" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Create a new account" textAlignment="JUSTIFY">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Button fx:id="submitButton" layoutX="429.0" layoutY="344.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="105.0" text="Submit" />
      <Button fx:id="backButton" layoutX="60.0" layoutY="344.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="40.0" prefWidth="105.0" text="&lt;&lt;Back" />
      <TextField fx:id="firstNameTextField" layoutX="60.0" layoutY="69.0" prefHeight="40.0" prefWidth="160.0" promptText="First Name" />
   </children>
</AnchorPane>
